   "source": [
    "from IPython.nbformat import v3, v4\n",
    "\n",
    "with open(\"BaF2.py\") as fpin:\n",
    "    text = fpin.read()\n",
    "\n",
    "nbook = v3.reads_py(text)\n",
    "nbook = v4.upgrade(nbook)  # Upgrade v3 to v4\n",
    "\n",
    "jsonform = v4.writes(nbook) + \"\\n\"\n",
    "with open(\"BaF2.ipynb\", \"w\") as fpout:\n",
    "    fpout.write(jsonform)"
   ]
  }
